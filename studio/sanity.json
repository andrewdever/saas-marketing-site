{
  "root": true,
  "project": {
    "name": "Studio"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "intl-input"
  ],
  "env": {
    "development": {
      "api": {
        "projectId": "rmixtpha",
        "dataset": "production"
      }
    },
    "production": {
      "api": {
        "projectId": "rmixtpha",
        "dataset": "development"
      }
    }
  },
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    },
    {
      "name": "part:@sanity/desk-tool/structure",
      "path": "./src/deskStructure.js"
    }
  ]
}
